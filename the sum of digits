
//the sum of digits 

#include<stdio.h>
int sumOfDigits(int);
int main (void)
{
    int number;
    
    scanf("%d",&number);
    sumOfDigits(number);
   
}
int sumOfDigits(int n)
{
    int sum =0;
    
    while (n!=0)
    {
    sum =sum +n %10;
    n = (n/10);
    }
     printf("%d",sum);
    return sum;
   
}
